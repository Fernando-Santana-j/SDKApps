                            <div class="inputs-containner">
                                <label for="product-config-new-price" class="lable-padrao">Preço do produto: </label>
                                <input type="text" class="input-padrao" id="product-config-new-price" placeholder="Preço">
                            </div>
                        </div>
                        <div id="produtos-config-new-style">
                            <h1 class="mini-title-col">Selecione o estilo: </h1>
                            <div id="produtos-config-new-style-content"></div>
                        </div>
                    </div>
                </div>
                <div class="linha"></div>
                <div id="produtos-config-estoque">
                    <h1 class="title-col">Configure o estoque : </h1>

